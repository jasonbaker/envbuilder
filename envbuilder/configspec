virtualenv-args = string(default='--no-site-packages --clear .')

    [[DEFAULT]]
    [[__many__]]
       # name is defined in config.py
       dir = string(default='$name')
       url = string(default='')
       checkout=force_list()
       ___many___ = list(default=list())

